<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mladenjovicic.vehicletender">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name=".VTApplication"
        android:allowBackup="true"
        android:icon="@drawable/carlogo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/carlogo"
        android:supportsRtl="true"
        android:theme="@style/Theme.VehicleTender"
        android:usesCleartextTraffic="true">
        <activity android:name=".showWinUserActivity"></activity>
        <activity android:name=".WinBidActivity" />
        <activity android:name=".UserBidTender" />
        <activity android:name=".TenderUseActivity2" />
        <activity android:name=".TenderActivity" />
        <activity android:name=".ui.TenderActivity" />
        <activity android:name=".UpdateUserActivity" />
        <activity android:name=".AdminPanelActivity" />
        <activity android:name=".LoginActivity"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>